<div class="page-header">
  <%= link_to new_calling_script_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Calling script
  <% end %>
  <h1>Listing calling_scripts</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th>Content</th>
            <th>Topic</th>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @calling_scripts) do |calling_script| %>
            <td><%= calling_script.content %></td>
            <td><%= calling_script.topic %></td>
            <td><%= link_to 'Show', calling_script %></td>
        <td><%= link_to 'Edit', edit_calling_script_path(calling_script) %></td>
        <td><%= link_to 'Destroy', calling_script, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
